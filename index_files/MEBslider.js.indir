  $("ol#resim").css("width", toplamwidth + "px");
  $("ol#resim").css("marginLeft", "-" + (liwidth - fireFox) + "px");
  var clone;
  if (toplamLi < 3) {
    clone = $("ol#resim li:last").clone();
    $("ol#resim").prepend(clone);
  } else {
    clone = $("ol#resim li:last").clone();
    $("ol#resim").prepend(clone);
    $("ol#resim li:last").remove();
  }
  $("#icerik li").mouseenter(function () {
    var index = $(this).index() + 1;
    var yon = liIndex - index;
    if (yon > 0) {
      if (liIndex == toplamLi && index == 1) {
        $("#ileri").click();
      } else {
        for (i = 0; i < yon; i++) {
          $("#geri").click();
        }
      }
    } else {
      if (index == toplamLi && liIndex == 1) {
        $("#geri").click();
      } else {
        for (i = 0; i < -yon; i++) {
          $("#ileri").click();
        }
      }
    }
    $.yaziEffect($(this).index());
  });
  $("#icerik li").click(function () {
    var index = $(this).index() + 1;
    var yon = liIndex - index;
    if (yon > 0) {
      if (liIndex == toplamLi && index == 1) {
        $("#ileri").click();
      } else {
        for (i = 0; i < yon; i++) {
          $("#geri").click();
        }
      }
    } else {
      if (index == toplamLi && liIndex == 1) {
        $("#geri").click();
      } else {
        for (i = 0; i < -yon; i++) {
          $("#ileri").click();
        }
      }
    }
    $.yaziEffect($(this).index());
  });
  $.yaziEffect = function (deger) {
    if (deger > slayt_sayisi - 1) {
      deger = 0;
    } else if (deger < 0) {
      deger = slayt_sayisi - 1;
    }
    $("#icerik li").removeClass("aktif");
    $("#icerik li:eq(" + deger + ")").addClass("aktif");
    $("#yazi li").hide();
    $("#yazi li:eq(" + deger + ")").show(yazi_efekt, yazi_efekt_sure);
    sayi = deger;
  };
  $("#ileri").click(function () {
    if (liIndex > toplamLi - 1) {
      liIndex = 0;
    }
    liIndex++;
    if (toplamLi < 3) {
      clone = $("ol#resim li").eq(1).clone();
    } else {
      clone = $("ol#resim li").eq(0).clone();
    }
    $("ol#resim").append(clone);
    $("ol#resim li").eq(0).remove();
    $("ol#resim").css("marginLeft", "0px");
    $("ol#resim").animate(
      { marginLeft: "-" + (liwidth - fireFox) + "px" },
      resim_efekt_sure
    );
    $.yaziEffect(sayi + 1);
    return false;
  });

  $("#geri").click(function () {
    if (liIndex == 1) {
      liIndex = toplamLi + 1;
    }
    liIndex--;
    if (toplamLi < 3) {
      clone = $("ol#resim li").eq(1).clone();
    } else {
      clone = $("ol#resim li:last").clone();
    }
    $("ol#resim").prepend(clone);
    if (toplamLi == 1) {
      $("ol#resim").append(clone);
      $("ol#resim li:last").remove();
      $("ol#resim").css("marginLeft", "-" + (liwidth - fireFox) + "px");
      $("ol#resim").animate({ marginLeft: "0px" }, resim_efekt_sure);
    } else {
      $("ol#resim li:last").remove();
      $("ol#resim").css("marginLeft", "-" + (liwidth - fireFox) * 2 + "px");
      $("ol#resim").animate(
        { marginLeft: "-" + (liwidth - fireFox) + "px" },
        resim_efekt_sure
      );
    }
    $.yaziEffect(sayi - 1);
    return false;
  });
