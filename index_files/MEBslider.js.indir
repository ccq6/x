      ileri_yer = "-" + ($("#aaa").height() / 2 + $("#yazi").height() + 90);
      yazi_yer = "-" + ($("#yazi").height() + 25);
    }
    if ($(window).width() < 463) {
      geri_yer = "-" + ($("#aaa").height() / 2 + $("#yazi").height() + 50);
      ileri_yer = "-" + ($("#aaa").height() / 2 + $("#yazi").height() + 80);
